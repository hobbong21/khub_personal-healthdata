# Development Environment Configuration
NODE_ENV=development

# Database Configuration
POSTGRES_DB=health_platform_dev
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Application Ports
FRONTEND_PORT=3000
BACKEND_PORT=5000

# JWT Configuration
JWT_SECRET=dev-jwt-secret-key-not-for-production
JWT_EXPIRES_IN=7d

# External API Keys (use test/development keys)
GOOGLE_CLOUD_PROJECT_ID=health-platform-dev
GOOGLE_APPLICATION_CREDENTIALS=./config/service-account-dev.json
OPENAI_API_KEY=sk-test-key

# File Upload Configuration
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads

# Email Configuration (use test SMTP)
SMTP_HOST=smtp.mailtrap.io
SMTP_PORT=2525
SMTP_USER=test-user
SMTP_PASS=test-password

# Security Configuration
BCRYPT_ROUNDS=10
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Monitoring Configuration
ENABLE_MONITORING=true
LOG_LEVEL=debug