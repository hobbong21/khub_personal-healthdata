# Database
DATABASE_URL="postgresql://username:password@localhost:5432/health_platform"

# JWT
JWT_SECRET="your-jwt-secret-key"

# Redis
REDIS_URL="redis://localhost:6379"

# Google Cloud (for OCR and NLP)
GOOGLE_APPLICATION_CREDENTIALS="path/to/service-account-key.json"

# OpenAI
OPENAI_API_KEY="your-openai-api-key"

# Google Fit API
GOOGLE_FIT_CLIENT_ID="your-google-fit-client-id"
GOOGLE_FIT_CLIENT_SECRET="your-google-fit-client-secret"
GOOGLE_FIT_REDIRECT_URI="http://localhost:3000/google-fit/callback"

# Apple Health (for iOS app integration)
APPLE_HEALTH_WEBHOOK_SECRET="your-apple-health-webhook-secret"

# Server Configuration
PORT=3001
NODE_ENV="development"

# File Upload
MAX_FILE_SIZE="10mb"
UPLOAD_PATH="./uploads"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100