REDIS_PORT=6379

# Backend
BACKEND_PORT=5001
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# Frontend
FRONTEND_PORT=80

# API URLs
VITE_API_BASE_URL=http://localhost:5001/api
